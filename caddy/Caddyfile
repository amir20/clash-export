https://clashstats.download,
https://www.clashleaders.com {
    root /app/clashleaders

    tls findamir@gmail.com

    proxy / unix:/tmp/gunicorn.sock {
        transparent
        except /favicon.ico /robots.txt /static
    }

    expires {
        match /static 1y
        match /clan/.*/short.json$ 1h
    }

    gzip
    log stdout
    errors stdout
}


clashleaders.com {
   redir https://www.clashleaders.com
}
