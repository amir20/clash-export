0.0.0.0, http://web {
    root * /app/clashleaders

    reverse_proxy / unix//tmp/gunicorn.sock     

    header /static {
        Cache-Control "public, max-age=31536000"
    }
    
    header / {
        -Server
    }
    encode gzip
    log {
        output stdout
        format single_field common_log
    }
}

localhost:8000 {
   reverse_proxy / unix:/tmp/gunicorn.sock 
}
