localhost:80 {
    root * /app/clashleaders
    file_server /static/*
    file_server /robots.txt

    @proxy {        
        not {
            path /static/*            
        }

        not {
            path /robots.txt            
        }
    }         

    reverse_proxy @proxy unix//tmp/gunicorn.sock   

    header / {
        -Server
    }
    
    encode gzip
    log {
        output stdout
    }
}
