localhost:80 {
    root * /app/clashleaders
    file_server /static/* 
    file_server /robots.txt

    reverse_proxy / unix//tmp/gunicorn.sock     

    header /static {
        Cache-Control "public, max-age=31536000"
    }

    header / {
        -Server
    }
    
    encode gzip
    log {
        output stdout
        format single_field common_log
    }
}
