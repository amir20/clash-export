{
    experimental_http3
}

http://localhost {
    root * /app/clashleaders
    file_server     

    @proxy {        
        not {
            path /static/*            
        }

        not {
            path /robots.txt            
        }
    }       

    reverse_proxy @proxy unix//tmp/gunicorn.sock   

    header / {
        -Server
    }
    
    encode gzip
    log {
        output stdout
    }
}
